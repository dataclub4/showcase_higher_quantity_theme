                    {% when 'buybox'%}
                    {% assign started_set = false %}          
                    {% assign related_products_collections = product.collections %}
                    {% assign related_products_collections_titles = product.collections | map: 'title'  %}
                      {% if related_products_collections_titles contains 'startersets' %}
                      {% else %}
                      {% for related_products_collection in related_products_collections %}
                        {%- paginate related_products_collection.products by related_products_collection.products_count -%}
                          {% assign related_products_array = related_products_collection.products %}
                        {%- endpaginate -%}
                        {% for related_product in related_products_array %}
                          {% if started_set == true %}
                            {% break %}  
                          {% endif %}
                          {% assign related_product_collections = related_product.collections | map: 'title' %}
                          
                          {% if related_product_collections contains 'startersets' %}
                            {% assign started_set = true %} 
                            {% assign started_set_product = related_product %}
                          {% endif %}
                        {% endfor %}
                      {% endfor %}
                      {% endif %}
                    {% if started_set_product %}
                    <div class="product-buybox">
                      <div class="product-buybox-wrapper">
                        <label class="product-buybox-label">
                          auch im starterset erhältlich
                        </label>
                        <div class="product-buybox-product">
                          <div class="product-buybox-product-image">
                            {{ started_set_product | image_url: width: 200 | image_tag }}
                          </div>
                          <div class="product-buybox-product-info">
                            <div class="product-buybox-product-title">
                              {{ started_set_product.title }}
                            </div>
                            <div class="product-buybox-product-price">
                                <div class="price theme-money">{%- render "price", price: started_set_product.price -%}</div>
                                {% if started_set_product.compare_at_price > started_set_product.price %}
                                <div class="was-price theme-money">{%- render "price", price: started_set_product.compare_at_price -%}</div>
                                {% endif %}
                            </div>
                            <a href="{{ started_set_product.url }}" class="button product-buybox-product-link">
                              Jetzt entdecken
                            </a>
                          </div>
                        </div>
                        <!-- <span class="product-buybox-linetext">test <b>test</b> test </span> -->
                      </div>
                      
                    </div>      
